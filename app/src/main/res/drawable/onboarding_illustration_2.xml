<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="240dp"
    android:height="240dp"
    android:viewportWidth="240"
    android:viewportHeight="240">
    
    <!-- Background gradient circle -->
    <circle
        android:cx="120"
        android:cy="120"
        android:r="100">
        <aapt:attr name="android:fillColor">
            <gradient
                android:angle="135"
                android:startColor="#f093fb"
                android:endColor="#f5576c"
                android:type="linear" />
        </aapt:attr>
    </circle>
    
    <!-- Rich text illustration - Document with formatting -->
    <rect
        android:width="80"
        android:height="100"
        android:x="80"
        android:y="70"
        android:rx="8"
        android:fillColor="#FFFFFF"
        android:strokeColor="#E0E0E0"
        android:strokeWidth="2"/>
    
    <!-- Formatted text lines -->
    <rect android:width="60" android:height="6" android:x="90" android:y="85" android:rx="1" android:fillColor="#2E2E2E"/>
    <rect android:width="25" android:height="6" android:x="90" android:y="95" android:rx="1" android:fillColor="#2E2E2E" android:alpha="0.8"/>
    <rect android:width="45" android:height="6" android:x="90" android:y="105" android:rx="1" android:fillColor="#f5576c"/>
    <rect android:width="35" android:height="6" android:x="90" android:y="115" android:rx="1" android:fillColor="#2E2E2E" android:alpha="0.6"/>
    <rect android:width="50" android:height="6" android:x="90" android:y="125" android:rx="1" android:fillColor="#2E2E2E"/>
    <rect android:width="40" android:height="6" android:x="90" android:y="135" android:rx="1" android:fillColor="#f093fb"/>
    
    <!-- Formatting toolbar -->
    <rect
        android:width="70"
        android:height="12"
        android:x="85"
        android:y="150"
        android:rx="6"
        android:fillColor="#F8F9FA"
        android:strokeColor="#E0E0E0"
        android:strokeWidth="1"/>
    
    <!-- Toolbar buttons -->
    <rect android:width="8" android:height="8" android:x="89" android:y="152" android:rx="2" android:fillColor="#667eea"/>
    <rect android:width="8" android:height="8" android:x="101" android:y="152" android:rx="2" android:fillColor="#764ba2"/>
    <rect android:width="8" android:height="8" android:x="113" android:y="152" android:rx="2" android:fillColor="#f5576c"/>
    <rect android:width="8" android:height="8" android:x="125" android:y="152" android:rx="2" android:fillColor="#4ECDC4"/>
    <rect android:width="8" android:height="8" android:x="137" android:y="152" android:rx="2" android:fillColor="#FFD700"/>
    
    <!-- Floating format symbols -->
    <text android:x="50" android:y="70" android:text="B" android:textSize="16" android:fillColor="#FFFFFF" android:fontFamily="serif" android:textStyle="bold"/>
    <text android:x="180" android:y="60" android:text="I" android:textSize="14" android:fillColor="#FFFFFF" android:fontFamily="serif" android:textStyle="italic"/>
    <text android:x="60" android:y="190" android:text="U" android:textSize="12" android:fillColor="#FFFFFF" android:fontFamily="serif"/>
    
</vector>
